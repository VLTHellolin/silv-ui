{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "lib": [
      "ESNext",
      "DOM",
      "DOM.Iterable"
    ],
    "allowJs": true,
    "noEmit": true,
    "incremental": true,
    "resolveJsonModule": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
  },
  "include": [
    "../**/*.ts",
    "../**/*.tsx",
    ".next/types/**/*.ts"
  ]
}
