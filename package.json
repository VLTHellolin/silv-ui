{
  "name": "@silv-ui/monorepo",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.4.1",
  "scripts": {
    "build": "pnpm --filter=./packages/* build",
    "build:preset": "pnpm --filter preset build",
    "build:react": "pnpm --filter react build",
    "prepare": "husky",
    "taze": "taze -Iwr",
    "taze:major": "taze major -Iwr"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@hellolin/eslint-config": "^0.3.2",
    "@types/mdx": "^2.0.13",
    "@types/node": "^22.13.4",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "eslint": "^9.20.1",
    "husky": "^9.1.7",
    "taze": "^18.6.0",
    "unocss": "^65.5.0"
  }
}
