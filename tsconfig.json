{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "jsx": "preserve",
    "strict": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "skipLibCheck": true,
  },
  "include": ["**/*.ts", "**/*.tsx"],
  "exclude": ["**/node_modules/**"],
}
